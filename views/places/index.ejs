<%- include("../partials/header") %>


    <header class="jumbotron">
        <div class="container text-center">
        <h1 class="display-4">Welcome!!!</h1>
    <p class="lead">View out unique instagrammable London places</p>
    <hr class="my-4">
   <p class="lead">
        <a class="btn btn-primary btn-lg" href="/places/new" role="button">Add a new LondonPlace</a>
    </p>
    </div>
</header>
<% places.forEach(place => { %>
    <div>
    <h4><li> <%=place.name %></li></h4>
    <img src= "<%= place.image %>">
    </div>
<% }) %>
<%- include("../partials/footer") %>